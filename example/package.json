{
  "name": "langchat-example",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "setup": "node -e \"console.log('Setting up example app...'); const fs = require('fs'); if (!fs.existsSync('.env')) { fs.copyFileSync('.env.example', '.env'); console.log('✅ .env created from template - Please configure with your Supabase credentials'); } else { console.log('✅ .env already exists'); }\"",
    "clean": "expo start --clear",
    "reset": "rm -rf node_modules && npm install",
    "postinstall": "npm run setup"
  },
  "dependencies": {
    "@expo/metro-runtime": "~5.0.4",
    "@react-native-async-storage/async-storage": "^1.24.0",
    "@langchain/langgraph-sdk": "^0.0.92",
    "@supabase/supabase-js": "^2.50.0",
    "expo": "~53.0.17",
    "expo-status-bar": "~2.2.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-web": "~0.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "react-native-builder-bob": "^0.40.8",
    "react-native-monorepo-config": "^0.1.9"
  },
  "private": true
}
